        "@id": "urn:uuid:question-climate-feedback",
        "text": "What is a positive feedback loop in climate science?",
        "acceptedAnswer": {
          "@type": "Answer",
          "@id": "urn:uuid:answer-climate-feedback",
          "text": "A positive feedback loop in climate science is a process where an initial change leads to additional changes that amplify or accelerate the original change, such as ice-albedo feedback where melting ice reduces surface reflectivity, leading to more warming."
        },
        "hasPart": {
          "@type": "SoftwareSourceCode",
          "@id": "urn:uuid:template-climate-feedback",
          "name": "Climate Feedback Loop Answer Template",
